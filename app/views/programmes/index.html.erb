<h1>Programmes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>No. Villages</th>
      <th>Top Change</th>
    </tr>
  </thead>

  <tbody>
    <% @programmes.each do |programme| %>
      <tr>
        <td><%= link_to programme.name, national_office_programme_path(programme, @natinal_office) %></td>
        <td><%#= programme.villages.count %>N/A</td>
        <td>N/A</td>
      </tr>
    <% end %>
  </tbody>
</table>