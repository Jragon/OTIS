<h1>Changes</h1>

<table>
  <thead>
    <tr>
      <th>Rank</th>
      <th>Name</th>
      <th>Score</th>
      <th>Average Ten Seed</th>
    </tr>
  </thead>

  <tbody>
    <% i = 1 %>
    <% @changes.each do |change| %>
      <tr>
        <td><%= i.ordinalize %><% i += 1 %></th>
        <td><%= change.name %></th>
        <td><%= change.score %></th>
        <td><%= change.average_ten_seed %></th>
      </tr>
    <% end %>
  </tbody>
</table>